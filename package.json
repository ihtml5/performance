    "build": "rollup -c --watch && http-server",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihtml5/performance.git"
  },
  "keywords": [
    "object",
    "scalpel",
    "processing"
  ],
  "author": "ihtml5",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ihtml5/performance/issues"
  },
